{
  "name": "ascii-update",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "compile": "./node_modules/.bin/wasm-pack build ./rust_src/ -d ../pkg --target web",
    "compile:release": "cd rust_src && ../node_modules/.bin/wasm-pack build --verbose -d ../pkg --target web --release",
    "generate-assets": "pwa-assets-generator --preset minimal-2023 public/logo.png"
  },
  "dependencies": {
    "@coddicat/vue-pinch-scroll-zoom": "^4.3.5",
    "@vueuse/core": "^10.7.2",
    "jakes-photo-booth": "file:./pkg",
    "rvfc-polyfill": "^1.0.7",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@rushstack/eslint-patch": "^1.3.3",
    "@vite-pwa/assets-generator": "^0.2.2",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.49.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.6",
    "vite": "^5.0.10",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0",
    "wasm-pack": "^0.12.1"
  }
}
