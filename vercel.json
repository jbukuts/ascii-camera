{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": false
  },
  "installCommand": "npm ci && ./bin/install-rust.sh",
  "buildCommand": "./bin/build-site.sh"
}
