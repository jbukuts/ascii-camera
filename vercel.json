{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ],
  "installCommand": "chmod +x ./bin/* && ./bin/install-rust.sh && npm ci",
  "buildCommand": "./bin/build-site.sh"
}
